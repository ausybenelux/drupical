{
  "config": {

    "vagrant_debugging": true,

    "box_type": "hashicorp/precise64",
    "box_name": "drupicaldev",
    "box_hostname": "drupicaldev",
    "box_gui": false,
    "box_dhcp": true,
    "box_static_ip": "10.0.0.1",
    "box_ram_cpu": "auto",
    "box_ram": "1G",
    "box_cpu": "2",

    "os_update": true,

    "vagrant_synced_folders": {
      "adminer" : {
        "source" : "./web-tools/adminer/",
        "target" : "/usr/share/adminer/",
        "type" : "nfs",
        "mount_options" : ["rw", "vers=3", "udp", "fsc", "actimeo=1"]
      },
      "siteinfo" : {
        "source" : "./web-tools/siteinfo/",
        "target" : "/usr/share/siteinfo/",
        "type" : "nfs",
        "mount_options" : ["rw", "vers=3", "udp", "fsc", "actimeo=1"]
      }
    },

    "solr": {
      "solr_install": false,
      "solr_version": "3.6.2",
      "solr_checksum": "537426dcbdd0dc82dd5bf16b48b6bcaf87cb4049c1245eea8dcb79eeaf3e7ac6"
    },

    "web_tools": {
      "web_tools_install": true,
      "url_base": "dev.drupicaldev.local",
      "tools": {
        "info": {
          "install": true,
          "alias": "info"
        },
        "adminer": {
          "install": true,
          "alias": "adminer"
        },
        "phpmemcachedadmin": {
          "install": true,
          "alias": "phpmemcachedadmin"
        },
        "opcachegui": {
          "install": true,
          "alias": "opcachegui"
        },
        "uprofiler_ui": {
          "install": false,
          "alias": "uprofiler"
        },
        "phpdoc": {
          "install": true,
          "alias": "phpdoc"
        },
        "munin": {
          "install": true,
          "alias": "munin"
        }
      }
    },

    "varnish_install": false,

    "testing_install": true,

    "vhosts": {},

    "php": {
