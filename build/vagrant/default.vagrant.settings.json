{
  "config": {

    "box_type": "hashicorp/precise64",
    "box_name": "drupicaldev",
    "box_hostname": "drupicaldev",
    "box_gui": false,

    "box_dhcp": true,
    "box_static_ip": "10.0.0.1", // ignored if box_dhcp == true

    "box_ram_cpu": "auto",
    "box_ram": "1G", // ignored if box_ram_cpu == auto
    "box_cpu": "2",  // ignored if box_ram_cpu == auto

    "os_update": false,

    "vagrant_debugging": true,

    "drupical": {

      "php" : {
        "php_version": "5.5",
        "enable_php_mcrypt": true,
        "enable_php_gd": true,
        "enable_php_opcode_cache": true,
        "enable_php_curl": true,
        "enable_php_memcache": true,
        "enable_php_drush": true,
        "enable_php_phing": true,
        "enable_php_composer": true,
        "enable_php_xdebug": true,
        "enable_php_xhprof": true,
        "enable_php_uprofiler": true
      },

      "solr": {
        "solr_install": false,
        "solr_version": "3.6.2",
        "solr_checksum": "537426dcbdd0dc82dd5bf16b48b6bcaf87cb4049c1245eea8dcb79eeaf3e7ac6"
      },

      "web_tools" : {
        "url_base" : "drupicaldev.local",
        "tools" : {
          "info" : {
            "install" : true,
            "alias" : "info"
          },
          "adminer" : {
            "install" : true,
            "alias" : "adminer"
          },
          "phpmemcachedadmin" : {
            "install" : true,
            "alias" : "phpmemcachedadmin"
          },
          "opcachegui" : {
            "install" : true,
            "alias" : "opcachegui"
          },
          "uprofiler_ui" : {
            "install" : true,
            "alias" : "xx"
          }
        }
      },

      "varnish_install": true

    },

    "vagrant_ports" : {
      "http" : {
        "host" : "80",
        "guest" : "80"
      },
      "https" : {
        "host" : "443",
        "guest" : "443"
      },
      "xdebug" : {
        "host" : "9000",
        "guest" : "9000"
      },
      "varnish" : {
        "host" : "8080",
        "guest" : "8080"
      },
      "mysql" : {
        "host" : "3306",
        "guest" : "3306"
      },
      "solr" : {
        "host" : "8390",
        "guest" : "8390"
      }
    },

    "vagrant_synced_folders" : {
    },

    "vhosts" : {
    }

  }
}
