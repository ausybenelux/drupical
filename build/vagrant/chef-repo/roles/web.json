{
  "name": "web",
  "description": "Web role",
  "json_class": "Chef::Role",
  "default_attributes": {
    "memcached": {
      "memory": "128"
    },
    "apache": {
      "version": "2.4",
      "default_modules": [
        "alias",
        "cgi",
        "deflate",
        "dir",
        "log_config",
        "mime",
        "rewrite",
        "setenvif"
      ],
      "mpm": "worker",
      "log_dir" : "/home/vagrant/drupical/logs/apache2"
    }
  },
  "chef_type": "role",
  "run_list": [
    "recipe[web::default]",
    "recipe[web::apache]",
    "recipe[web::apache_repo]",
    "recipe[web-memcached::default]"
  ],
  "env_run_lists": {
  }
}
